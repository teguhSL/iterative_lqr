<?xml version="1.0" ?>
<robot name="support"  xmlns:xacro="http://www.ros.org/wiki/xacro">
    <link name="base_footprint">
      <inertial>
        <origin
            xyz="0 0 0"
            rpy="0 0 0" />
        <mass
            value="0.1" />
        <inertia
            ixx="1"
            ixy="0"
            ixz="0"
            iyy="1"
            iyz="0"
            izz="1" />            
      </inertial>
    </link>

    <link name="support">
        <visual>
            <geometry>
                <mesh filename="support.obj"/>
            </geometry>
      <material name="panda_white"/>
        </visual>
        <collision>
            <geometry>
                <mesh filename="support.obj"/>
            </geometry>
        </collision>
    </link>

    <joint
        name="base_to_support"
        type="fixed">
      <origin
          xyz="0.0 0.0 0.1125" />
      <parent
          link="base_footprint" />
      <child
          link="support" />
    </joint>
  <material name="panda_white">
    <color rgba="1. 1. 1. 1."/>
  </material>
</robot>

